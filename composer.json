{
    "name": "mubeen/laravel-user-crud",
    "description": "Laravel package for user CRUD operations and authentication",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mubeen\\LaravelUserCrud\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Your Name",
            "email": "youremail@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/auth": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "suggest": {
        "laravel/sanctum": "Required for API authentication using Sanctum",
        "laravel/passport": "Required for API authentication using Passport",
        "tymon/jwt-auth": "Required for API authentication using JWT"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mubeen\\LaravelUserCrud\\LaravelUserCrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 